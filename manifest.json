{
  "manifest_version": 2,
  "name": "dark mode for chrome",
  "description": "This extension by simon",
  "version": "1.0",
  "browser_action": {
    "default_icon": "myimg.png",
    "default_popup": "popup.html",
    "icons": { "128": "myimg.png" }
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
  "permissions": [
      "tabs",
      "<all_urls>"
    ]

}
